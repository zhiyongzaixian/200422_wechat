/* 
	stylus
	： 1.省略大括号
		 2.省略封号和冒号
		 3.支持样式嵌套
 
 */
.indexContainer .header {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.indexContainer .header .logo {
  width: 140rpx;
  height: 40rpx;
  margin: 10rpx 30rpx;
}
.indexContainer .header .search {
  width: 420rpx;
  height: 60rpx;
}
.indexContainer .header button {
  width: 144rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #bb2c08;
  padding: 0 4rpx;
}

